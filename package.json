{
    "name": "@star-insure/sdk",
    "description": "The SDK for Star Insure client apps with shared helper functions and TypeScript definitions.",
    "author": "alexclark_nz",
    "version": "5.0.11",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/star-insure/sdk"
    },
    "main": "dist/index.js",
    "typings": "dist/index.d.ts",
    "files": [
        "dist",
        "src"
    ],
    "engines": {
        "node": ">=10"
    },
    "scripts": {
        "dev": "tsdx watch",
        "start": "tsdx watch",
        "build": "tsdx build",
        "test": "TZ='Pacific/Auckland' tsdx test --passWithNoTests",
        "lint": "tsdx lint",
        "prepare": "tsdx build",
        "size": "size-limit",
        "analyze": "size-limit --why",
        "publish": "np"
    },
    "husky": {
        "hooks": {
            "pre-commit": "tsdx lint"
        }
    },
    "prettier": {
        "printWidth": 80,
        "semi": true,
        "singleQuote": true,
        "trailingComma": "es5"
    },
    "module": "dist/sdk.esm.js",
    "size-limit": [
        {
            "path": "dist/starinsure-sdk.cjs.production.min.js",
            "limit": "10 KB"
        },
        {
            "path": "dist/starinsure-sdk.esm.js",
            "limit": "10 KB"
        }
    ],
    "devDependencies": {
        "@size-limit/preset-small-lib": "^7.0.8",
        "@types/lodash-es": "^4.17.6",
        "@types/react": "^18.0.19",
        "@types/react-dom": "^18.0.6",
        "@types/uuid": "^8.3.4",
        "husky": "^8.0.1",
        "np": "^9.2.0",
        "react": "^18.2.0",
        "size-limit": "^7.0.8",
        "tsdx": "^0.14.1",
        "tslib": "^2.4.0",
        "typescript": "^3.9.10"
    },
    "dependencies": {
        "@headlessui/react": "^1.6.3",
        "@inertiajs/react": "^1.0.0",
        "@tailwindcss/forms": "^0.5.9",
        "@tailwindcss/typography": "^0.5.15",
        "classnames": "^2.3.2",
        "date-fns": "^2.28.0",
        "lodash-es": "^4.17.21",
        "react-dom": "^18.2.0",
        "react-icons": "^4.11.0",
        "react-select": "^5.8.0",
        "react-tooltip": "^5.25.0",
        "uuid": "^9.0.0"
    },
    "peerDependencies": {
        "react": "^18.2.0"
    }
}
