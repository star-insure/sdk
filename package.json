{
    "name": "@star-insure/sdk",
    "description": "The SDK for Star Insure client apps with shared helper functions and TypeScript definitions.",
    "author": "alexclark_nz",
    "version": "1.1.5",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/star-insure/sdk"
    },
    "main": "index.js",
    "typings": "index.d.ts",
    "files": [
        "dist",
        "src"
    ],
    "engines": {
        "node": ">=10"
    },
    "scripts": {
        "dev": "tsc",
        "build": "tsc"
    },
    "husky": {
        "hooks": {
            "pre-commit": "tsdx lint"
        }
    },
    "prettier": {
        "printWidth": 80,
        "semi": true,
        "singleQuote": true,
        "trailingComma": "es5"
    },
    "module": "dist/starinsure-sdk.esm.js",
    "size-limit": [
        {
            "path": "dist/starinsure-sdk.cjs.production.min.js",
            "limit": "10 KB"
        },
        {
            "path": "dist/starinsure-sdk.esm.js",
            "limit": "10 KB"
        }
    ],
    "devDependencies": {
        "@size-limit/preset-small-lib": "^7.0.8",
        "@types/lodash-es": "^4.17.6",
        "@types/react": "^18.0.19",
        "@types/react-dom": "^18.0.6",
        "@types/uuid": "^8.3.4",
        "husky": "^8.0.1",
        "np": "^7.6.1",
        "size-limit": "^7.0.8",
        "tsdx": "^0.14.1",
        "tslib": "^2.4.0",
        "typescript": "^3.9.10"
    },
    "dependencies": {
        "@headlessui/react": "^1.6.3",
        "@inertiajs/inertia": "^0.11.0",
        "@inertiajs/inertia-react": "^0.8.0",
        "date-fns": "^2.28.0",
        "lodash-es": "^4.17.21",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "uuid": "^9.0.0"
    }
}
