{
    "name": "@star-insure/sdk",
    "description": "The SDK for Star Insure client apps with shared helper functions and TypeScript definitions.",
    "author": "alexclark_nz",
    "version": "0.3.22",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/star-insure/sdk"
    },
    "main": "dist/index.js",
    "typings": "dist/index.d.ts",
    "files": [
        "dist",
        "src"
    ],
    "engines": {
        "node": ">=10"
    },
    "scripts": {
        "dev": "tsdx watch",
        "start": "tsdx watch",
        "build": "tsdx build",
        "test": "tsdx test",
        "lint": "tsdx lint",
        "prepare": "tsdx build",
        "size": "size-limit",
        "analyze": "size-limit --why"
    },
    "husky": {
        "hooks": {
            "pre-commit": "tsdx lint"
        }
    },
    "prettier": {
        "printWidth": 80,
        "semi": true,
        "singleQuote": true,
        "trailingComma": "es5"
    },
    "module": "dist/starinsure-sdk.esm.js",
    "size-limit": [
        {
            "path": "dist/starinsure-sdk.cjs.production.min.js",
            "limit": "10 KB"
        },
        {
            "path": "dist/starinsure-sdk.esm.js",
            "limit": "10 KB"
        }
    ],
    "devDependencies": {
        "@size-limit/preset-small-lib": "^7.0.8",
        "husky": "^8.0.1",
        "np": "^7.6.1",
        "size-limit": "^7.0.8",
        "tsdx": "^0.14.1",
        "tslib": "^2.4.0",
        "typescript": "^3.9.10"
    },
    "dependencies": {
        "@inertiajs/inertia": "^0.11.0",
        "date-fns": "^2.28.0"
    }
}
